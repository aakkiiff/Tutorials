
pipeline {
    agent any

    stages {
        stage('CLEANUP WORKSPACE'){
            steps{
                script{
                    cleanWs()
                }
            }
        }
        }
    post{
        success {
            withCredentials([string(credentialsId: 'google_token', variable: 'toekn')]) {
            hangoutsNotify(
            message: "PIPELINE: $env.JOB_NAME has completed SUCCESSFULLY.<br>BUILD NUMBER: $env.BUILD_NUMBER,
            token: env.toekn
            )
        }
        }
    }
}
